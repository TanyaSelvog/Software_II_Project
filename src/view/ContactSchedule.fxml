<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" style="-fx-background-color: f9df69;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ContactScheduleController">

    <Label layoutX="334.0" layoutY="38.0" text="Contact Schedule Report">   <font>
        <Font name="Verdana Bold" size="24.0" />   </font>
    </Label>
   <TableView fx:id="contactTable" layoutX="348.0" layoutY="137.0" prefHeight="200.0" prefWidth="578.0">
     <columns>
       <TableColumn fx:id="apptIDCol" prefWidth="75.0" text="Appt ID" />
         <TableColumn fx:id="titleCol" prefWidth="75.0" text="Title" />
       <TableColumn fx:id="typeCol" prefWidth="75.0" text="Type" />
         <TableColumn fx:id="descCol" prefWidth="75.0" text="Description" />
         <TableColumn fx:id="startCol" prefWidth="75.0" text="Start Date &amp; Time" />
         <TableColumn fx:id="endCol" prefWidth="75.0" text="End Date &amp; Time" />
         <TableColumn fx:id="custIDCol" prefWidth="75.0" text="Customer ID" />
     </columns>
   </TableView>
   <ComboBox fx:id="contactCB" layoutX="143.0" layoutY="169.0" prefHeight="29.0" prefWidth="174.0" />
   <Button fx:id="generateBtn" layoutX="143.0" layoutY="283.0" mnemonicParsing="false" onAction="#onGenerateBtn" text="Generate Report">
      <font>
         <Font name="Verdana" size="18.0" />
      </font>
   </Button>
   <Label layoutX="54.0" layoutY="172.0" text="Contact">
      <font>
         <Font name="Verdana" size="18.0" />
      </font>
   </Label>

   <Button fx:id="reportsBtn" layoutX="679.0" layoutY="453.0" mnemonicParsing="false" onAction="#onReportsBtn" text="Reports">
   <font>
        <Font name="Verdana" size="18.0" />
    </font>
</Button>

   <Button fx:id="homeBtn" layoutX="789.0" layoutY="453.0" mnemonicParsing="false" onAction="#onHomeBtn" text="Home">
    <font>
        <Font name="Verdana" size="18.0" />
    </font>
</Button>

</AnchorPane>
